generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Users {
  Id           Int     @id(map: "PK__Users__3214EC075BA28823") @default(autoincrement())
  Email        String  @unique(map: "UQ__Users__A9D105341AEAA1E6") @db.NVarChar(255)
  PasswordHash String? @db.NVarChar(255)
  Role         String? @db.NVarChar(255)
  Active       Boolean
}





